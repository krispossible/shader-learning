  slideText: "Shader \"Custom/Hologram\" \r\n{\r\n\r\nProperties \r\n{    \r\n  _RimColor
    (\"Rim Color\", Color) = (0.0,0.5,0.5,0.0)\r\n  _RimPower (\"Rim Power\", Range(0.5,8.0))
    = 3.0\r\n}\r\n\r\nSubShader \r\n{\r\n\r\nTags\r\n{\r\n   \"Queue\" = \"Transparent\"\r\n}\r\n\r\nPass\r\n{\r\n
    \   ZWrite On\r\n    ColorMask 0\r\n}\r\n\r\nCGPROGRAM\r\n    #pragma surface
    surf Lambert alpha:fade\r\n    \r\n    float4 _RimColor;\r\n    float _RimPower;\r\n
    \   \r\n    struct Input\r\n    {\r\n        float3 viewDir;\r\n    };\r\n    \r\n
    \   \r\nvoid surf (Input IN, inout SurfaceOutput o) \r\n{    \r\n\r\nhalf rim
    = 1.0 - saturate(dot(normalize(IN.viewDir), o.Normal));  \r\n      \r\no.Emission
    =_RimColor.rgb * pow(rim, _RimPower)*10;\r\n        \r\no.Alpha = pow(rim, _RimPower);\r\n
    \               \r\n}\r\nENDCG\r\n}\r\n\tFallBack \"Diffuse\"\r\n}"
