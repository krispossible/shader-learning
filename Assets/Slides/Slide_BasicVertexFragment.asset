  slideText: "Shader \"Custom/BasicVf\"\r\n{\r\n\r\nProperties\r\n{\r\n   _MainTex
    (\"Texture\", 2D) = \"white\" {}\r\n}\r\n\r\nSubShader\r\n{\r\n\r\nTags \r\n{
    \r\n   \"RenderType\"=\"Opaque\" \r\n}\r\n\r\nLOD 100\r\n\r\n// If we use vertex
    and fragment together, need to put in a pass\r\n\r\nPass\r\n{\r\nCGPROGRAM\r\n
    \   #pragma vertex vert\r\n    #pragma fragment frag\r\n    \r\n    #include \"UnityCG.cginc\"\r\n\r\n
    \   struct appdata\r\n    {\r\n        float4 vertex : POSITION;\r\n        float2
    uv : TEXCOORD0;\r\n    };\r\n\r\n    struct v2f\r\n    {\r\n        float2 uv
    : TEXCOORD0;\r\n        float4 vertex : SV_POSITION;\r\n    };\r\n\r\n    sampler2D
    _MainTex;\r\n    float4 _MainTex_ST;\r\n\r\n    // world space, x and z coords\r\n
    \   // runs on every vertex\r\n    \r\n    v2f vert (appdata v)\r\n    {\r\n      v2f
    o;\r\n      o.vertex = UnityObjectToClipPos(v.vertex);\r\n      o.uv = TRANSFORM_TEX(v.uv,
    _MainTex);\r\n      return o;\r\n    }\r\n\r\n    // screen space, x and y coords\r\n
    \   // runs on every pixel\r\n    \r\n    fixed4 frag (v2f i) : SV_Target\r\n
    \   {\r\n        fixed4 col = tex2D(_MainTex, i.uv);\r\n        return col;\r\n
    \   }\r\n    \r\nENDCG\r\n    }\r\n  }\r\n}\r\n"
